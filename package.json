{
  "name": "@sc-voice/scv-static",
  "version": "0.1.48",
  "description": "SuttaCentral Voice static website library",
  "main": "index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "vue-cookie": "^1.1.4",
    "vue-details": "^1.0.9"
  },
  "devDependencies": {
    "@mdi/js": "^5.8.55",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/vuetify": "^1.11.2",
    "axios": "^0.21.1",
    "eslint-plugin-html": "^4.0.5",
    "fibers": "^5.0.0",
    "memo-again": "^0.3.12",
    "mocha": "^7.1.1",
    "push-dir": "^0.4.1",
    "sass-loader": "^8",
    "scv-bilara": "^1.4.66",
    "should": "^11.2.0",
    "vue-material-design-icons": "^4.11.0"
  },
  "scripts": {
    "build": "scripts/build.js",
    "build:examples": "scripts/build.js",
    "build:nuxt": "nuxt build",
    "test": "npm run test:verbose",
    "test:test": "mocha -w --watch-ignore node_modules,.git,local -R min --inline-diffs -g TEST './@(src|test)/**/*.js'",
    "test:verbose": "mocha --inline-diffs",
    "test:watch": "mocha -w --inline-diffs",
    "dev": "nuxt",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "repository": {
    "tag": "git"
  },
  "bin": {},
  "keywords": [
    "SuttaCentral",
    "SuttaCentral Voice",
    "EBT",
    "Voice",
    "Buddha",
    "Pali",
    "translation",
    "Javascript",
    "bilara-data",
    "Bilara"
  ],
  "author": "Karl Lew",
  "homepage": "https://github.com/sc-voice/scv-bilara",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/vue@0.1.13"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
