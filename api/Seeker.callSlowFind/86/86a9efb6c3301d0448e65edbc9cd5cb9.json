        "segMap": {
          "an6.43:0.3": {
            "scid": "an6.43:0.3",
            "pli": "43. Nāgasutta ",
            "en": "43. The <span class=\"scv-matched\">Giant</span> ",
            "matched": true
          },
          "an6.43:3.1": {
            "scid": "an6.43:3.1",
            "pli": "Tena kho pana samayena rañño pasenadissa kosalassa seto nāma nāgo mahātūriyatāḷitavāditena pubbakoṭṭhakā paccuttarati. ",
            "en": "Now, at that time King Pasenadi had a <span class=\"scv-matched\">giant</span> bull elephant called “White”. It emerged from the eastern gate to the beating and playing of musical instruments. ",
            "matched": true
          },
          "an6.43:3.3": {
            "scid": "an6.43:3.3",
            "pli": "“abhirūpo vata bho rañño nāgo; dassanīyo vata bho rañño nāgo; pāsādiko vata, bho, rañño nāgo, kāyupapanno vata bho rañño nāgo”ti. ",
            "en": "“The royal <span class=\"scv-matched\">giant</span> is so handsome! The royal <span class=\"scv-matched\">giant</span> is so good-looking! The royal <span class=\"scv-matched\">giant</span> is so lovely! The royal <span class=\"scv-matched\">giant</span> has such a huge body!” ",
            "matched": true
          },
          "an6.43:3.6": {
            "scid": "an6.43:3.6",
            "pli": "‘nāgo vata bho nāgo’ti, udāhu aññampi kañci mahantaṁ brahantaṁ kāyupapannaṁ jano disvā evamāha: ‘nāgo vata bho nāgo’”ti? ",
            "en": "‘A <span class=\"scv-matched\">giant</span>, such a <span class=\"scv-matched\">giant</span>’? Or do they say it when they see any other creatures with huge, formidable bodies?” ",
            "matched": true
          },
          "an6.43:3.8": {
            "scid": "an6.43:3.8",
            "pli": "‘nāgo vata bho nāgo’ti. ",
            "en": "‘A <span class=\"scv-matched\">giant</span>, such a <span class=\"scv-matched\">giant</span>!’ ",
            "matched": true
          },
          "an6.43:3.14": {
            "scid": "an6.43:3.14",
            "pli": "‘nāgo vata, bho, nāgo’ti. ",
            "en": "‘A <span class=\"scv-matched\">giant</span>, such a <span class=\"scv-matched\">giant</span>!’ ",
            "matched": true
          },
          "an6.43:3.15": {
            "scid": "an6.43:3.15",
            "pli": "Api ca, udāyi, yo sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya āguṁ na karoti kāyena vācāya manasā, tamahaṁ ‘nāgo’ti brūmī”ti. ",
            "en": "But Udāyī, one who does nothing monstrous by way of body, speech, and mind is who I call a ‘<span class=\"scv-matched\">giant</span>’ in this world with its gods, Māras, and Brahmās, this population with its ascetics and brahmins, its gods and humans.” ",
            "matched": true
          },
          "an6.43:4.3": {
            "scid": "an6.43:4.3",
            "pli": "api ca, udāyi, yo sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya āguṁ na karoti kāyena vācāya manasā, tamahaṁ ‘nāgo’ti brūmīti. ",
            "en": "‘But Udāyī, one who does nothing monstrous by way of body, speech, and mind is who I call a “<span class=\"scv-matched\">giant</span>” in this world with its gods, Māras, and Brahmās, this population with its ascetics and brahmins, its gods and humans.’ ",
            "matched": true
          },
          "an6.43:8.1": {
            "scid": "an6.43:8.1",
            "pli": "Sabbe accarucī nāgo, ",
            "en": "That <span class=\"scv-matched\">giant</span> outshines all, ",
            "matched": true
          },
          "an6.43:8.3": {
            "scid": "an6.43:8.3",
            "pli": "Sabbesaṁ nāganāmānaṁ, ",
            "en": "Of all those named ‘<span class=\"scv-matched\">giant</span>’, ",
            "matched": true
          },
          "an6.43:9.1": {
            "scid": "an6.43:9.1",
            "pli": "Nāgaṁ vo kittayissāmi, ",
            "en": "I’ll extol the <span class=\"scv-matched\">giant</span> for you, ",
            "matched": true
          },
          "an6.43:9.4": {
            "scid": "an6.43:9.4",
            "pli": "pādā nāgassa te duve. ",
            "en": "are two feet of the <span class=\"scv-matched\">giant</span>. ",
            "matched": true
          },
          "an6.43:10.3": {
            "scid": "an6.43:10.3",
            "pli": "Saddhāhattho mahānāgo, ",
            "en": "Faith is the <span class=\"scv-matched\">giant</span>’s trunk, ",
            "matched": true
          },
          "an6.43:12.3": {
            "scid": "an6.43:12.3",
            "pli": "Gacchaṁ samāhito nāgo, ",
            "en": "The <span class=\"scv-matched\">giant</span> is serene when walking, ",
            "matched": true
          },
          "an6.43:12.4": {
            "scid": "an6.43:12.4",
            "pli": "ṭhito nāgo samāhito. ",
            "en": "the <span class=\"scv-matched\">giant</span> is serene when standing, ",
            "matched": true
          },
          "an6.43:13.1": {
            "scid": "an6.43:13.1",
            "pli": "Seyyaṁ samāhito nāgo, ",
            "en": "the <span class=\"scv-matched\">giant</span> is serene when lying down, ",
            "matched": true
          },
          "an6.43:13.2": {
            "scid": "an6.43:13.2",
            "pli": "nisinnopi samāhito; ",
            "en": "and when sitting, the <span class=\"scv-matched\">giant</span> is serene. ",
            "matched": true
          },
          "an6.43:13.3": {
            "scid": "an6.43:13.3",
            "pli": "Sabbattha saṁvuto nāgo, ",
            "en": "The <span class=\"scv-matched\">giant</span> is restrained everywhere: ",
            "matched": true
          },
          "an6.43:13.4": {
            "scid": "an6.43:13.4",
            "pli": "esā nāgassa sampadā. ",
            "en": "this is the accomplishment of the <span class=\"scv-matched\">giant</span>. ",
            "matched": true
          },
          "an6.43:19.3": {
            "scid": "an6.43:19.3",
            "pli": "Viññassanti mahānāgā, ",
            "en": "Great <span class=\"scv-matched\">giant</span>s will understand ",
            "matched": true
          },
          "an6.43:19.4": {
            "scid": "an6.43:19.4",
            "pli": "nāgaṁ nāgena desitaṁ. ",
            "en": "what the <span class=\"scv-matched\">giant</span> taught the <span class=\"scv-matched\">giant</span>. ",
            "matched": true
          },
          "an6.43:20.3": {
            "scid": "an6.43:20.3",
            "pli": "Sarīraṁ vijahaṁ nāgo, ",
            "en": "the <span class=\"scv-matched\">giant</span>, giving up his body, ",
            "matched": true
          }
